<UserControl
	x:Class="StarRatingsControl.StarControl"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Height="Auto"
	Width="Auto"
	xmlns:starratingscontrol="clr-namespace:StarRatingsControl">
	<Grid
		x:Name="gdStar">
		<Path
			Name="starForeground"
			Fill="#FF808080"
			Stroke="#00FFFFFF"
			StrokeThickness="1"
			Data="M5,0 L4,4 L0,4 L3,7 L2,11 L5,9 L6,9 L9,11 L8,7 L11,4 L7,4 L6,0" />
		<Rectangle
			x:Name="mask"
			Margin="0" />
		<Path
			Name="starOutline"
			Fill="#00FFFFFF"
			Stroke="#00FFFFFF"
			StrokeThickness="1"
			Data="M5,0 L4,4 L0,4 L3,7 L2,11 L5,9 L6,9 L9,11 L8,7 L11,4 L7,4 L6,0" />
	</Grid>
</UserControl>
